<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServletProject</display-name>
  <servlet>
    <servlet-name>ImageServlet</servlet-name>
    <servlet-class>com.urbantrip.controllers.images.ImagesController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageServlet</servlet-name>
    <url-pattern>/images</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShowTour</servlet-name>
    <servlet-class>com.urbantrip.controllers.Tour.ShowTour</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowTour</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.urbantrip.controllers.User.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DestinationServlet</servlet-name>
    <servlet-class>com.urbantrip.controllers.Destination.DestinationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DestinationServlet</servlet-name>
    <url-pattern>/destination</url-pattern>
  </servlet-mapping>
