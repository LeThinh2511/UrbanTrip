    <servlet-name>DeleteDestinationServlet</servlet-name>
    <url-pattern>/layout/admin/deleteDestination</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddDestinationServlet</servlet-name>
    <servlet-class>com.urbantrip.controllers.Destination.AddDestinationController</servlet-class>
