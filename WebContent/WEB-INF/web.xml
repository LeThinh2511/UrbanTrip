  <servlet-mapping>
    <servlet-name>AddDestinationServlet</servlet-name>
    <url-pattern>/layout/admin/addDestination</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index</welcome-file>
